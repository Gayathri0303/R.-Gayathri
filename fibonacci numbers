b = int(input("How many terms? "))
f = 0
s = 1
count = 0
if b <= 0:
   print("Please enter a positive integer")
elif b == 1:
   print("Fibonacci sequence upto",b,":")
   print(f)
else:
   print("Fibonacci sequence:")
   while count < b:
       print(f)
       x = f + s
       f = s
       s = x 
       count += 1
